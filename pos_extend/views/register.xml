<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    
    <template id="assets_backend_index" name="index pos ui" inherit_id="point_of_sale.index">
        <xpath expr="//head" position="inside">           
            <script type="text/javascript" src="/pos_extend/static/src/libs/js/jquery-barcode-last.min.js"/>
        </xpath>
    </template>
    
    <template id="assets_backend" name="pos_extend assets" inherit_id="point_of_sale.assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/pos_extend/static/src/js/model.js"/>
            <script type="text/javascript" src="/pos_extend/static/src/js/order.js"/>
            <script type="text/javascript" src="/pos_extend/static/src/js/screens.js"/>
            <script type="text/javascript" src="/pos_extend/static/src/js/payment_pad.js"></script>
            <script type="text/javascript" src="/pos_extend/static/src/js/pos_payment.js"/>
            <script type="text/javascript" src="/pos_extend/static/src/js/database.js"/>
            <link rel="stylesheet" href="/pos_extend/static/src/css/pos_extend.css"/>
            <link rel="stylesheet" href="/pos_extend/static/src/css/payment_pad.css"/>
        </xpath>
    </template>

</odoo>
